macro_macro_paste: macro_macro_paste{
compatible = "zmk,behavior-macro";
label = "macro_macro_paste";
#binding-cells = <0>;
bindings = <&macro_press &kp LCTRL>, <&macro_tap &kp DOT>, <&macro_release &kp LCTRL>;
};
