macro_greater_than: macro_greater_than{
compatible = "zmk,behavior-macro";
label = "macro_greater_than";
#binding-cells = <0>;
bindings = <&macro_press &kp LCTRL &kp LSHFT>, <&macro_tap &kp U &kp 3 &kp E>, <&macro_release &kp LCTRL &kp LSHFT>;
};
