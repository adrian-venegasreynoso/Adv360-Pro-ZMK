compatible = "zmk,behavior-macro";
label = "macro_less_than";
#binding-cells = <0>;
bindings = <&macro_press &kp LCTRL &kp LSHFT>, <&macro_tap &kp U &kp 3 &kp C>, <&macro_release &kp LCTRL &kp LSHFT>;
};
